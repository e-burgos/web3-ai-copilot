{
  "name": "ai-gateway",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ai-gateway/src",
  "projectType": "application",
  "tags": ["type:app", "scope:backend"],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ai-gateway",
        "main": "apps/ai-gateway/src/index.ts",
        "tsConfig": "apps/ai-gateway/tsconfig.app.json",
        "assets": ["apps/ai-gateway/src/assets"],
        "platform": "node",
        "bundle": true,
        "format": ["cjs"],
        "outputFileName": "index.cjs",
        "skipTypeCheck": true
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "ai-gateway:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
